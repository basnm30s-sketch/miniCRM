{
  "name": "imanage",
  "version": "1.0.0",
  "description": "iManage - Car Rental CRM System",
  "author": "Your Name",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "build": "next build",
    "build:electron": "node scripts/build-electron.js",
    "build:server": "tsc -p tsconfig.server.json --skipLibCheck --noCheck",
    "build:all": "npm run build:electron && npm run build:server",
    "dev": "concurrently \"npm run api:dev\" \"next dev\"",
    "api:dev": "tsx api/server.ts",
    "lint": "eslint .",
    "start": "next start",
    "electron:dev": "concurrently \"cross-env NODE_ENV=development next dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build:all && npm run rebuild:electron:optional && electron-builder",
    "electron:build-win": "npm run build:all && npm run rebuild:electron:optional && electron-builder --win",
    "electron:build-win-unpacked": "npm run build:all && npm run rebuild:electron:optional && electron-builder --win --dir",
    "electron:build-mac": "npm run build:all && npm run rebuild:electron:optional && electron-builder --mac",
    "electron:build-linux": "npm run build:all && npm run rebuild:electron:optional && electron-builder --linux",
    "build:static": "next build",
    "test": "jest",
    "test:e2e": "npx playwright test --project=Core",
    "test:e2e:core": "npx playwright test --project=Core",
    "test:e2e:extended": "npx playwright test --project=Extended",
    "test:e2e:ui": "npx playwright test --ui",
    "prebuild": "node scripts/test-runner.js",
    "rebuild:electron": "electron-rebuild --force --only better-sqlite3",
    "rebuild:electron:optional": "electron-rebuild --force --only better-sqlite3 || echo \"Rebuild skipped (file may be locked - electron-builder will handle rebuild automatically)\"",
    "demo:populate": "tsx scripts/populate-demo-data.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@vercel/analytics": "1.3.1",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "cors": "^2.8.5",
    "date-fns": "4.1.0",
    "docx": "^9.5.1",
    "electron-is-dev": "^3.0.1",
    "embla-carousel-react": "8.5.1",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "html2canvas": "latest",
    "input-otp": "1.4.1",
    "jspdf": "latest",
    "lucide-react": "^0.454.0",
    "multer": "^2.0.2",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "9.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "latest",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "uuid": "^13.0.0",
    "vaul": "^0.9.9",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.9",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "postcss": "^8.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.9",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "build": {
    "appId": "com.imanage.crm",
    "productName": "iManage",
    "files": [
      "out/**/*",
      "electron/**/*",
      "dist-server/**/*",
      "data/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "asarUnpack": [
      "**/node_modules/better-sqlite3/**/*"
    ],
    "npmRebuild": true,
    "win": {
      "target": "nsis",
      "forceCodeSigning": false,
      "signAndEditExecutable": false
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}